<% if session[:user_id] %>
  <%= link_to "Log Out", "/logout" %>
<% else %>
  <%= button_to "Create a New User", register_path, method: :get, data: { turbo: false } %> 
  <%= link_to "Log In", login_path %>
<% end %>
<h3>Existing Users:</h3>
<% @users.each do |user| %>
  <li><%= user.email %></li>
<% end %>